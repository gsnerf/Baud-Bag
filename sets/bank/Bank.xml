    <Frame name="BaudBagContainer2_1" mixin="BaudBagFirstBankMixin,BaudBagBankContainerMixin" inherits="BaudBagFirstContainerTemplate">
		<Frames>
			<Frame parentKey="BagsFrame" inherits="BaudBagBagsFrameTemplate" mixin="BaudBagBankBagsFrameMixin" frameStrata="MEDIUM">
				<Frames>
					<Frame parentKey="PurchaseFrame" setAllPoints="true">
						<Frames>
							<Button parentKey="PurchaseButton" inherits="BankPanelPurchaseButtonScriptTemplate,UIPanelButtonTemplate" text="BANKSLOTPURCHASE">
								<Size>
									<AbsDimension x="0" y="21" />
								</Size>
								<Anchors>
									<Anchor point="BOTTOM" x="0" y="5" />
									<Anchor point="LEFT" x="5" y="0" />
									<Anchor point="RIGHT" x="-5" y="0" />
								</Anchors>
							</Button>
							<Frame name="$parentMoneyFrame" parentKey="MoneyFrame" inherits="SmallMoneyFrameTemplate">
								<Anchors>
									<Anchor point="BOTTOM" x="0" y="25" />
									<Anchor point="LEFT" x="5" y="0" />
									<Anchor point="RIGHT" x="-5" y="0" />
								</Anchors>
								<Scripts>
									<OnLoad>
										SmallMoneyFrame_OnLoad(self);
										MoneyFrame_SetType(self, "STATIC");
									</OnLoad>
								</Scripts>
							</Frame>
						</Frames>
					</Frame>
                    <Frame parentKey="TabSettingsMenu" inherits="BankPanelTabSettingsMenuTemplate" mixin="BaudBagBankTabSettingsMixin" hidden="true">
						<Anchors>
							<Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="BOTTOMLEFT" x="-5" y="-5"/>
						</Anchors>
					</Frame>
				</Frames>
				<Scripts>
					<OnShow method="OnShow" />
				</Scripts>
			</Frame>
			<Button name="$parentDepositButton" parentKey="DepositButton" mixin="BaudBagBankDepositButtonMixin">
				<Size x="16" y="16" />
				<Anchors>
					<Anchor point="TOPRIGHT" relativeTo="$parentSearchButton" relativePoint="TOPLEFT" x="0" y="0"/>
				</Anchors>
				<NormalTexture file="Interface\EncounterJournal\UI-EncounterJournalTextures">
					<TexCoords left="0.73046875" right="0.82421875" top="0.61816406" bottom="0.66015625"/>
				</NormalTexture>
				<PushedTexture file="Interface\EncounterJournal\UI-EncounterJournalTextures">
					<TexCoords left="0.63281250" right="0.72656250" top="0.61816406" bottom="0.66015625"/>
				</PushedTexture>
				<HighlightTexture file="Interface\Buttons\ButtonHilight-Square" alphaMode="ADD">
					<Size x="12" y="13"/>
					<Anchors>
						<Anchor point="CENTER" x="0" y="0"/>
					</Anchors>
				</HighlightTexture>
				<Scripts>
					<OnClick method="OnClick" />
					<OnEnter method="OnEnter" />
					<OnLeave function="GameTooltip_Hide"/>
				</Scripts>
			</Button>
		</Frames>
        <Scripts>
			<OnLoad method="OnContainerLoad" />
			<OnShow method="OnBankShow" />
			<OnEvent method="OnBankEvent" />
			<OnHide method="OnBankHide" />
		</Scripts>
	</Frame>

	<Frame name="BaudBagBankContainerTemplate" inherits="BaudBagContainerTemplate" mixin="BaudBagBankContainerMixin" virtual="true">
		<Frames>
			<Button name="$parentDepositButton" parentKey="DepositButton" mixin="BaudBagBankDepositButtonMixin">
				<Size x="16" y="16" />
				<Anchors>
					<Anchor point="TOPRIGHT" relativeTo="$parentSearchButton" relativePoint="TOPLEFT" x="0" y="0"/>
				</Anchors>
				<NormalTexture file="Interface\EncounterJournal\UI-EncounterJournalTextures">
					<TexCoords left="0.73046875" right="0.82421875" top="0.61816406" bottom="0.66015625"/>
				</NormalTexture>
				<PushedTexture file="Interface\EncounterJournal\UI-EncounterJournalTextures">
					<TexCoords left="0.63281250" right="0.72656250" top="0.61816406" bottom="0.66015625"/>
				</PushedTexture>
				<HighlightTexture file="Interface\Buttons\ButtonHilight-Square" alphaMode="ADD">
					<Size x="12" y="13"/>
					<Anchors>
						<Anchor point="CENTER" x="0" y="0"/>
					</Anchors>
				</HighlightTexture>
				<Scripts>
					<OnClick method="OnClick" />
					<OnEnter method="OnEnter" />
					<OnLeave function="GameTooltip_Hide"/>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnLoad method="OnContainerLoad" />
			<OnShow method="OnContainerShow" />
			<OnEvent method="OnContainerEvent" />
		</Scripts>
	</Frame>
</Ui>